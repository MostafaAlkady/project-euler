   ],
   "source": [
    "flag = True\n",
    "n = 1\n",
    "while flag:\n",
    "    if partition(n) % 1000000 == 0:\n",
    "        print(n, partition(n))\n",
    "        flag = False\n",
    "        break\n",
    "    else:\n",
    "        n += 1"
